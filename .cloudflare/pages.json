{
  "buildCommand": "npm run build:cloudflare",
  "outputDirectory": ".next",
  "compatibility": {
    "node": "18"
  },
  "nodejsCompatibility": {
    "enabled": true
  },
  "routes": [
    { "pattern": "/api/*", "script": "functions/api/[[path]].js" },
    { "pattern": "/*", "serve": "/.next" }
  ]
}
